{
	"name": "Pinsir",
	"version": "1.0",
	"description": "Open pinned tabs when new window is created.",
	"manifest_version": 3,
	"permissions": ["storage", "tabs"],
	"background": {
		"service_worker": "background.js"
	},
  "action": {
    "default_title": "Pinsir",
    "default_popup": "popup.html"
  }
}
